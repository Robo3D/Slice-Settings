{
    "id": "robo_r2",
    "version": 2,
    "name": "Robo R2",
    "inherits": "fdmprinter",
    "metadata": {
        "visible": true,
        "author": "Robo",
        "manufacturer": "Robo",
        "category": "Robo",
        "file_formats": "text/x-gcode",
		    "platform": "robor2.stl",
        "platform_offset": [-117, -22, -135]
    },

    "overrides": {
        "machine_name": { "default_value": "Robo R2" },
        "machine_start_gcode": {
            "default_value": "; -- START GCODE --\n;-- end of START GCODE --" },
        "machine_end_gcode": {
            "default_value": "; -- END GCODE --\n; -- end of END GCODE --" },
	    	"machine_width": { "default_value": 197 },
        "machine_depth": { "default_value": 197 },
        "machine_height": { "default_value": 250 },
        "machine_heated_bed": { "default_value": true },
        "machine_filament_park_distance": { "value": "machine_heat_zone_length + 5" },
        "machine_nozzle_heat_up_speed": { "default_value": 1.4 },
        "machine_nozzle_cool_down_speed": { "default_value": 0.8 },
        "machine_nozzle_size": {
            "default_value": 0.4,
            "minimum_value": "0.001" },
        "machine_extruder_count": { "default_value": 1 },
        "extruder_prime_pos_abs": { "default_value": true },
        "prime_tower_position_x": { "value": "machine_depth - max(extruderValue(adhesion_extruder_nr, 'brim_width') * extruderValue(adhesion_extruder_nr, 'initial_layer_line_width_factor') / 100 if adhesion_type == 'brim' else (extruderValue(adhesion_extruder_nr, 'raft_margin') if adhesion_type == 'raft' else (extruderValue(adhesion_extruder_nr, 'skirt_gap') if adhesion_type == 'skirt' else 0)), max(extruderValues('travel_avoid_distance'))) - max(extruderValues('support_offset')) - sum(extruderValues('skirt_brim_line_width')) - 30" },
        "prime_tower_wipe_enabled": { "default_value": false },
        "prime_blob_enable": { "enabled": true },
        "material_diameter": { "default_value": 1.75 },
        "machine_center_is_zero": { "default_value": false },
        "machine_gcode_flavor": { "default_value": "RepRap (Marlin/Sprinter)" },
        "adhesion_type": { "default_value": "raft" },
        "machine_acceleration": { "default_value": 500},
        "acceleration_print": { "default_value": 500},
        "acceleration_infill": { "default_value": 500},
        "acceleration_wall": { "default_value": 500},
        "acceleration_wall_0": { "default_value": 500},
        "acceleration_wall_x": { "default_value": 500},
        "acceleration_topbottom": { "default_value": 500},
        "acceleration_support": { "default_value": 500},
        "acceleration_support_infill": { "default_value": 500},
        "acceleration_support_interface": { "default_value": 500},
        "acceleration_prime_tower": { "default_value": 500},
        "acceleration_travel": { "default_value": 500, "value": "acceleration_print if magic_spiralize else 500"},
        "acceleration_layer_0": { "default_value": 500},
        "acceleration_print_layer_0": { "default_value": 500},
        "acceleration_travel_layer_0": { "default_value": 500},
        "acceleration_skirt_brim": { "default_value": 500},
        "brim_width": { "value": "3" },
        "bottom_thickness": { "default_value": 1 },
        "cool_fan_full_at_height": { "value": "layer_height_0 + 4 * layer_height" },
        "cool_fan_speed": { "value": "50" },
        "cool_fan_speed_max": { "value": "100" },
        "cool_min_speed": { "value": "5" },
        "infill_line_width": { "value": "round(line_width * 0.5 / 0.35, 2)" },
        "infill_overlap": { "value": "0.06" },
        "infill_pattern": { "value": "'triangles'" },
        "infill_wipe_dist": { "value": "0" },
        "jerk_enabled": { "value": "True" },
        "jerk_layer_0": { "value": "jerk_topbottom" },
        "jerk_prime_tower": { "value": "math.ceil(jerk_print * 15 / 25)" },
        "jerk_print": { "value": "25" },
        "jerk_support": { "value": "math.ceil(jerk_print * 15 / 25)" },
        "jerk_support_interface": { "value": "jerk_topbottom" },
        "jerk_topbottom": { "value": "math.ceil(jerk_print * 5 / 25)" },
        "jerk_wall": { "value": "math.ceil(jerk_print * 10 / 25)" },
        "jerk_wall_0": { "value": "math.ceil(jerk_wall * 5 / 10)" },
        "layer_height_0": { "value": "round(machine_nozzle_size / 1.3, 2)" },
        "layer_start_x": { "value": "sum(extruderValues('machine_extruder_start_pos_x')) / len(extruderValues('machine_extruder_start_pos_x'))" },
        "layer_start_y": { "value": "sum(extruderValues('machine_extruder_start_pos_y')) / len(extruderValues('machine_extruder_start_pos_y'))" },
        "line_width": { "value": "round(machine_nozzle_size * 0.875, 3)" },
        "machine_min_cool_heat_time_window": { "value": "15" },
        "default_material_print_temperature": { "value": "200" },
        "material_print_temperature_layer_0": { "value": "material_print_temperature + 5" },
        "material_bed_temperature": { "maximum_value": "80" },
        "material_bed_temperature_layer_0": { "maximum_value": "80" },
        "material_standby_temperature": { "value": "100" },
        "multiple_mesh_overlap": { "value": "0" },
        "prime_tower_enable": { "default_value": false },
        "raft_airgap": { "value": "0.26" },
        "raft_base_thickness": { "value": "0.3" },
        "raft_base_line_spacing": { "value": "3" },
        "raft_base_line_width": { "value": "1" },
        "raft_base_speed": { "value": "15.0" },
        "raft_interface_speed": { "value": "45.0" },
        "raft_interface_line_width": { "value": "0.8" },
        "raft_interface_thickness": { "value": "0.2" },
        "raft_jerk": { "value": "jerk_layer_0" },
        "raft_margin": { "value": "10" },
        "raft_surface_layers": { "value": "2" },
        "retraction_enable": { "value": true},
        "retraction_amount": { "value": "1.5" },
        "retraction_count_max": { "value": "10" },
        "retraction_extrusion_window": { "value": "1" },
        "retraction_hop": { "default": "0.075", "visible": true },
        "retraction_hop_enabled": { "value": "True" },
        "retraction_hop_only_when_collides": { "value": "True" },
        "retraction_min_travel": { "value": "5" },
        "retraction_prime_speed": { "value": "10" },
        "skin_overlap": { "value": "10" },
        "skin_line_width": { "value": "math.ceil(machine_nozzle_size *2)"},
        "speed_infill": { "default_value": "speed_print" },
        "speed_layer_0": { "value": "20" },
        "speed_prime_tower": { "value": "speed_topbottom" },
        "speed_print": { "value": "5" },
        "speed_support": { "value": "speed_wall_0" },
        "speed_support_interface": { "value": "speed_topbottom" },
        "speed_topbottom": { "value": "math.ceil(speed_print * 20 / 35)" },
        "speed_travel": { "value": "100" },
        "speed_wall": { "value": "math.ceil(speed_print * 30 / 35)" },
        "speed_wall_0": { "value": "math.ceil(speed_wall * 20 / 30)" },
        "speed_wall_x": { "value": "speed_wall" },
        "support_enable": { "default": false },
        "support_angle": { "value": "45" },
        "support_pattern": { "value": "'lines'" },
        "support_use_towers": { "value": "False" },
        "support_xy_distance": { "value": "wall_line_width_0 * 2.5" },
        "support_xy_distance_overhang": { "value": "wall_line_width_0" },
        "support_z_distance": { "value": "0.16" },
        "switch_extruder_prime_speed": { "value": "10" },
        "switch_extruder_retraction_amount": { "value": "5" },
        "top_bottom_thickness": { "value": "1" },
        "travel_avoid_distance": { "value": "3" },
        "wall_0_inset": { "value": "0" },
        "wall_line_count": { "value": "3" },
        "wall_line_width_x": { "value": "round(line_width * 0.3 / 0.35, 2)" },
        "wall_thickness": { "value": "1" }
    }
}
